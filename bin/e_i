#!/bin/bash

set -e
grep  -Eo '/.+.java:\[.*error.*' "$1"|sort -r|uniq| while read str;do
	a="${str%%]*}"
	msg="${str:$[ ${#a} + 2 ]}"
	file="${a%:*}"
	file0="${a%:*}"
	line="${a//*[}"
	line="${line%,*}"
	if [ "$DEBUG" != "" ]; then
		echo "$line"
		echo "file >$file< "
		echo "msg >$msg<"
		echo "line >$line<"
	fi
	file="${file#*\/}"
	file="${file#*\/}"
	file="${file#*\/}"
	file="${file#*\/}"
	file="${file#*\/}"
	insert_line "$file" "$line" "// FIXME:${str:${#file0}}"
	#exit
done
